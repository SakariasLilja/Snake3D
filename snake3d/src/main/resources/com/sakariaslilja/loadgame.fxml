<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.geometry.Insets?>

<VBox spacing="20.0" fx:controller="com.sakariaslilja.controllers.LoadGameController" >
    <children>
        <HBox spacing="25.0" >
            <children>
                <Button fx:id="goback" text="Back" onAction="#gotoMainPage" />
                <Label text="Load a Previous Game" />
            </children>
            <padding>
                <Insets up="5.0" down="5.0" right="5.0" left="5.0" />
            </padding>
        </HBox>
    </children>
    <padding>
        <Insets up="10.0" down="10.0" right="10.0" left="10.0" />
    </padding>
</VBox>