<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.geometry.Insets?>

<VBox fx:controller="com.sakariaslilja.controllers.NewGameController" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" >
    <children>
        <HBox spacing="25.0" >
            <children>
                <Button fx:id="goback" text="Back" onAction="#gotoMainPage" />
                <Label text="Create a New Game" />
            </children>
            <padding>
                <Insets top="5.0" bottom="5.0" right="5.0" left="5.0" />
            </padding>
        </HBox>
    </children>
    <padding>
        <Insets top="10.0" bottom="10.0" right="10.0" left="10.0" />
    </padding>
</VBox>